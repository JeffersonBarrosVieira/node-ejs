{
    "version": 2,
    "builds": [
        {
            "src": "/index.js",
            "use": "@now/node-server"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "/index.js"
        },
        {
            "src": "/home",
            "methods": [
                "POST",
                "GET"
            ],
            "dest": "/views/home.ejs"
        }
    ]
}